{
  "compilerOptions": {
    "moduleResolution": "node",
    "noImplicitAny": false,
    "esModuleInterop": true,
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "lib": ["es2015", "es2016", "es2017.object", "dom"],
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "declaration": true,
    "outDir": "lib",
    "typeRoots": ["node_modules/@types", "typings"],
    "resolveJsonModule": true,
    "baseUrl": "./"
  }
}
